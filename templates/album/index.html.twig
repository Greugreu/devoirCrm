{% extends 'base.html.twig' %}

{% block body %}

    <div class="example-wrapper">
        <table class="table-striped">
            <thead>
            <th scope="col">Id</th>
            <th scope="col">Title</th>
            </thead>
            <tbody>
            {% for data in datas %}
                <tr>
                    <th scope="row">{{ data.id }}</th>
                    <td>{{ data.title }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>

{% endblock %}
